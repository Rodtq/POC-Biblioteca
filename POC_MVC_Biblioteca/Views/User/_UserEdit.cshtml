@model POC_MVC_Biblioteca.ViewModels.UserViewModel
<div id="edit-user-partial-container" class="container-fluid">
  @using (@Html.BeginForm("EditUser", "User"))
  {
      <div id="IdSmartDiv" class="row">
        <div class="col-lg-2">
          @Html.Label("Código/Id:")
        </div>
        <div class="col-lg-10">
          @Html.TextBoxFor(m => m.IdSmart)
        </div>
      </div>
      <div id="NomeDiv" class="row">
        <div class="col-lg-2">
          @Html.Label("Nome:")
        </div>
        <div class="col-lg-10">
          @Html.TextBoxFor(m => m.Name)
        </div>
      </div>
      <div id="EmailDiv" class="row">
        <div class="col-lg-2">
          @Html.Label("E-Mail:")
        </div>
        <div class="col-lg-10">
          @Html.TextBoxFor(m => m.Email)
        </div>
      </div>
      <div id="AreaDepartamentoDiv" class="row">
        <div class="col-lg-2">
          @Html.Label("Area/Departamento:")
        </div>
        <div class="col-lg-10">
          @Html.TextBoxFor(m => m.AreaDepartament)
        </div>
      </div>
      <div id="FuncaoDiv" class="row">
        <div class="col-lg-2">
          @Html.Label("Função:")
        </div>
        <div class="col-lg-10">
          @Html.TextBoxFor(m => m.Function)
        </div>
      </div>
      <div id="RamalDiv" class="row">
        <div class="col-lg-2">
          @Html.Label("Ramal:")
        </div>
        <div class="col-lg-10">
          @Html.TextBoxFor(m => m.ExtensionLine)
        </div>
      </div>
      <div id="GerenteDiv" class="row">
        <div class="col-lg-2">
          @Html.Label("Gerente:")
        </div>
        <div class="col-lg-10">
          @Html.TextBoxFor(m => m.Manager)
        </div>
      </div>
      <input type="submit" value="Finalizar">
  }
</div>
<script type="text/javascript">
  $(document).ready(function () {
    GenericPostHandler($("#editUser-fmr"), $("#container-partials"))
  });
</script>
































